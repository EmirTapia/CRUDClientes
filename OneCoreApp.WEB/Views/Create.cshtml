@model OneCore.API.Models.Cliente

<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form asp-controller="Pacientes" asp-action="Index" method="post">
                <div class="form-group">
                    <input asp-for="Nombre" id="nombre" name="nombre" placeholder="Ingrese el nombre del cliente" type="text" required="required" class="form-control">
                </div>
                <div class="form-group">
                    <input asp-for="CorreoContacto" id="correo" name="correo" placeholder="Ingrese correo de contacto" type="text" required="required" class="form-control">
                </div>
                <div class="form-group">
                    <input asp-for="Rfc" id="rfc" name="rfc" placeholder="Ingrese RFC de cliente" type="text" required="required" class="form-control">
                </div>
                <div class="form-group">
                    <input asp-for="Direccion" id="text" name="text" placeholder="Ingrese la dirección del cliente" type="text" required="required" class="form-control">
                </div>
                <div class="form-group">
                    <button name="submit" type="submit" class="btn btn-primary">Guardar</button>
                </div>
            </form>
        </div>
    </div>
</div>